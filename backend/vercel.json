{
    "version": 2,
    "builds": [
        {
            "src": "src/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH",
                "OPTIONS"
            ],
            "headers": [
                {
                    "source": "/:path*",
                    "has": [
                        {
                            "type": "query",
                            "key": "authorized"
                        }
                    ],
                    "headers": [
                        {
                            "key": "x-authorized",
                            "value": "true"
                        }
                    ]
                }
            ]
        }
    ]
}