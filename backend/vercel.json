{
    "version": 2,
    "builds": [
        {
            "src": "src/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH",
                "OPTIONS"
            ],
            "headers": [
                {
                    "source": "/",
                    "headers": [
                        {
                            "key": "Access-Control-Allow-Origin",
                            "value": "*"
                        },
                        {
                            "key": "Access-Control-Allow-Methods",
                            "value": "*"
                        },
                        {
                            "key": "Access-Control-Allow-Headers",
                            "value": "*"
                        }
                    ]
                }
            ]
        }
    ]
}